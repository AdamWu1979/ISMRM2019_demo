      <name>Filter</name>
      <dll>gadgetron_inatilib</dll>
      <classname>FilterGadget</classname>
      <property>   <name>filterType</name>   <value>1</value>   </property>
   </gadget -->

   <gadget>
      <name>FFT</name>
      <dll>gadgetron_mricore</dll>
      <classname>FFTGadget</classname>
   </gadget>

   <gadget>
      <name>FieldMapPython</name>
      <dll>gadgetron_python</dll>
      <classname>PythonGadget</classname>
      <property><name>python_path</name>              <value></value></property>
      <property><name>python_module</name>            <value>fieldMapCompute</value></property>
      <property><name>python_class</name>             <value>PhaseDifference</value></property>
   </gadget>

   <gadget>
      <name>Extract</name>
      <dll>gadgetron_mricore</dll>
      <classname>ExtractGadget</classname>
   </gadget>

